{
  "name": "fabricashape",
  "version": "1.0.2",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/fabricashape.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/fabricashape.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/i3d2c/fabricashape.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Krasimir Tsonev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/i3d2c/fabricashape/issues"
  },
  "homepage": "https://github.com/i3d2c/fabricashape",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "braces": "^2.3.1",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "jsdom": "15.2.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "yargs": "^14.2.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "fabric": "^3.4.0"
  }
}
